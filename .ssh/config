Host *
    #connection sharing
    ControlMaster auto
    ControlPath /tmp/ssh_mux_%h_%p_%r
    ControlPersist 3h
    #use local keys on remote machines, http://git.io/vO2A6
    #ForwardAgent yes
    #keep the connection alive for finnecky servers
    TCPKeepAlive no
    ServerAliveInterval 60
    ServerAliveCountMax 10

Host *.pagekite.me
    CheckHostIP no
    ProxyCommand /bin/nc -X connect -x %h:443 %h %p

Host h*.javier.io
    User chilicuil
    CheckHostIP no
    ProxyCommand /bin/nc -X connect -x %h:443 %h %p

Host *.javier.io msg.sh *.minos.io
    User admin

Host buzz
    Port 1003
    #LocalCommand echo buzz
    #PermitLocalCommand yes

Host ccand.cc #hostgator
    Port 2222
    PubkeyAuthentication no

Host bitbucket.org
    IdentityFile ~/.ssh/correo.io.id_rsa
