Host *
    #use my local keys on remote machines
    ForwardAgent yes
    #keep the connection alive for finnecky servers, and kill it when the
    #connection has died
    ServerAliveInterval 15
    ServerAliveCountMax 3

Host buzz
    Port 1003
    #LocalCommand echo buzz
    #PermitLocalCommand yes

Host *.pagekite.me
    CheckHostIP no
    ProxyCommand /bin/nc -X connect -x %h:443 %h %p

Host home.javier.io
    CheckHostIP no
    ProxyCommand /bin/nc -X connect -x %h:443 %h %p

Host h.javier.io
    CheckHostIP no
    ProxyCommand /bin/nc -X connect -x %h:443 %h %p

Host bitbucket.org
    IdentityFile ~/.ssh/correo.io.id_rsa

Host ccand.cc #hostgator
    Port 2222
    PubkeyAuthentication no
